CREATE TABLE venues (
id SERIAL,
name varchar(256),
address varchar(256),
city varchar(256),
state varchar(256),
lat real,
lng real);

CREATE TABLE specials (
id SERIAL,
description varchar(256),
day varchar(80),
endTime varchar(12),
startTime varchar(12),
lat real,
lng real,
price numeric,
venue varchar(256),
venueId integer);
